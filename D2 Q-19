import cv2
import numpy as np
from sklearn.decomposition import PCA
from matplotlib import pyplot as plt

# Load the image
image = cv2.imread('C:\Users\HP\Pictures\Screenshots\Screenshot (292).png', cv2.IMREAD_GRAYSCALE)

# Reshape the image to a 2D array of pixels
original_shape = image.shape
image_2d = image.reshape(-1, 1)

# Apply PCA
pca = PCA(n_components=1)
pca_result = pca.fit_transform(image_2d)
pca_image = pca_result.reshape(original_shape)

# Display the images
plt.figure(figsize=(10, 5))
plt.subplot(1, 2, 1)
plt.title('Original Image')
plt.imshow(image, cmap='gray')

plt.subplot(1, 2, 2)
plt.title('PCA Image')
plt.imshow(pca_image, cmap='gray')
plt.show()
